{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "compose": {
      "error": "This resource does not support generation in the manifest."
    },
    "orderSubGraph": {
      "type": "project.v1",
      "path": "../../quick-start.Orders/quick-start.Orders.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "Kestrel__Endpoints__http__Url": "http://*:{orderSubGraph.bindings.http.targetPort}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{compose-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "orderSubGraph"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 5002
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 5002
        }
      }
    },
    "productSubGraph": {
      "type": "project.v1",
      "path": "../../quick-start.Products/quick-start.Products.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "Kestrel__Endpoints__http__Url": "http://*:{productSubGraph.bindings.http.targetPort}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{compose-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "productSubGraph"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 5003
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 5003
        }
      }
    },
    "gateway": {
      "type": "project.v1",
      "path": "../../quick-start.Gateway/quick-start.Gateway.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{gateway.bindings.http.targetPort};{gateway.bindings.external.targetPort}",
        "ORDERSUBGRAPH_HTTP": "{orderSubGraph.bindings.http.url}",
        "services__orderSubGraph__http__0": "{orderSubGraph.bindings.http.url}",
        "PRODUCTSUBGRAPH_HTTP": "{productSubGraph.bindings.http.url}",
        "services__productSubGraph__http__0": "{productSubGraph.bindings.http.url}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{compose-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "gateway"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        },
        "external": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "port": 8080,
          "targetPort": 8000
        }
      }
    },
    "compose-dashboard": {
      "type": "container.v1",
      "image": "mcr.microsoft.com/dotnet/nightly/aspire-dashboard:latest",
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 18888,
          "external": true
        },
        "otlp-grpc": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 18889
        },
        "otlp-http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 18890
        }
      }
    }
  }
}